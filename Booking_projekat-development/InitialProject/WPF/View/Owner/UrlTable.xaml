<Window x:Class="InitialProject.View.UrlTable"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:InitialProject.View"
        mc:Ignorable="d"
        Title="UrlTable" Height="300" Width="500">
    <Grid>

        <DataGrid ItemsSource="{Binding Images }"  Margin="0,0,0,10">
            <DataGrid.Columns>
                <DataGridTextColumn Header="URL" Binding="{Binding Url}" Width="500" ></DataGridTextColumn>
            </DataGrid.Columns>
        </DataGrid>

    </Grid>
</Window>
