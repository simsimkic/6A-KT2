<Window x:Class="InitialProject.View.Guide.TourStatistic"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:InitialProject.View.Guide"
        mc:Ignorable="d"
        Title="EditTourPointForm" Height="189" Width="300" WindowStyle="ToolWindow" WindowStartupLocation="CenterScreen">
    <Grid Margin="0,0,0,-1">
        <ComboBox Name ="OrderComboBox" IsTextSearchEnabled="True" StaysOpenOnEdit="True" IsEditable="True"
                      ItemsSource="{Binding Years}" SelectedValue="{Binding Path=Year, UpdateSourceTrigger=PropertyChanged}"  HorizontalAlignment="Left" Width ="78" SelectedIndex="0" Margin="10,18,0,100"/>
        <Label Name="MostVisitedTour" Margin="114,28,-114,39"></Label>
        <Button Content="Pronadji" Click="FindTourButton" HorizontalAlignment="Left" Margin="10,82,0,0" VerticalAlignment="Top" Width="79" Height="25" RenderTransformOrigin="0.484,-0.454"/>

    </Grid>
</Window>